{
  "name": "joinmarket-api-ts",
  "version": "0.0.1",
  "description": "OpenAPI TypeScript types and client for JoinMarket",
  "private": true,
  "license": "MIT",
  "scripts": {
    "test": "npm run test:ts",
    "test:ts": "tsc --noEmit",
    "openapi:fetch-schema": "wget https://joinmarket-org.github.io/joinmarket-clientserver/api/wallet-rpc.yaml -O ./src/jm-wallet-rpc/jm-wallet-rpc.yaml",
    "openapi:generate": "npx openapi-typescript ./src/jm-wallet-rpc/jm-wallet-rpc.yaml -o ./src/jm-wallet-rpc/jm-wallet-rpc.d.ts",
    "example": "npx ts-node ./example.ts"
  },
  "devDependencies": {
    "@types/node": "^22.8.5",
    "openapi-typescript": "^7.4.1",
    "openapi-typescript-helpers": "^0.0.13",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "i18next": "^23.16.4",
    "openapi-fetch": "^0.12.2"
  }
}
